  <servlet>
    <servlet-name>LoginDemo</servlet-name>
    <servlet-class>com.demo.LoginDemo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginDemo</servlet-name>
    <url-pattern>/ServiceLogin</url-pattern>
  </servlet-mapping>
</web-app>