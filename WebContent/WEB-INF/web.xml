  <welcome-file-list>
    <welcome-file>Saveerloginpage.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Saveerloginpage</servlet-name>
    <servlet-class>com.demo.Saveerloginpage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Saveerloginpage</servlet-name>
